{
  "name": "mobile-store-v2",
  "version": "2.0.0",
  "description": "Upgraded version for 13Team Website",
  "main": "dist/index.js",
  "scripts": {
    "start": "pnpm migration:run && node dist/index.js",
    "start:dev": "pnpm exec ts-node-dev --respawn --transpile-only --no-notify --clear src/index.ts",
    "build": "pnpm rimraf dist/ && pnpm tsc",
    "typeorm": "pnpm exec typeorm-ts-node-commonjs -d ./src/config/data-source.ts",
    "migration:generate": "pnpm typeorm migration:generate ./src/migration/migration",
    "migration:run": "pnpm typeorm migration:run ",
    "migration:revert": "pnpm typeorm migration:revert",
    "test": "pnpm jest",
    "test:watch": "pnpm jest --watch --verbose",
    "lint:staged": "lint-staged",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "npx prettier --write"
    ]
  },
  "nodemonConfig": {
    "ignore": "frontend/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apollo-server": "^3.12.0",
    "apollo-server-core": "^3.12.0",
    "apollo-server-express": "^3.12.0",
    "bcrypt": "^5.1.0",
    "class-validator": "^0.14.0",
    "classnames": "^2.3.2",
    "compression": "^1.7.4",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "graphql": "^15.8.0",
    "inversify": "^6.0.1",
    "jest-mock-extended": "^3.0.4",
    "jsonwebtoken": "^9.0.0",
    "method-override": "^3.0.0",
    "moment": "^2.29.4",
    "nodemailer": "^6.9.1",
    "passport": "^0.5.3",
    "passport-google-oauth2": "^0.2.0",
    "pg": "^8.10.0",
    "pg-mem": "^2.6.12",
    "qs": "^6.11.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.4.1",
    "type-graphql": "1.2.0-rc.1",
    "typeorm": "^0.3.15",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^16.18.23",
    "@types/nodemailer": "^6.4.7",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.1",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  }
}
